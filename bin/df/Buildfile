DIRDEP= gnu/lib/csu gnu/lib/libgcc include lib/csu/MACHINE_ARCH lib/libc lib/libutil stage sys/MACHINE_ARCH/include sys/bsm sys/sys sys/ufs/ufs
.include <bsd.prog.mk>
