# $FreeBSD$


.PATH: ${BSDSRCTOP}/lib/ncurses/ncurses
INCDIR = ${STAGEDIR}/usr/include
COMMONDIR =

INCS = \
	ncurses_cfg.h \
	ncurses_def.h

AWK ?= awk

ncurses_def.h:
	AWK=${AWK} sh ${NCURSES_DIR}/include/MKncurses_def.sh \
	    ${NCURSES_DIR}/include/ncurses_defs > ncurses_def.h

.include <bsd.headers.mk>
