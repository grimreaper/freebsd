# $FreeBSD$

MOUNT = ${BSDSRCTOP}/sbin/mount

.PATH: ${MOUNT}

PROG = mount_udf

SRCS = \
	getmntopts.c \
	mount_udf.c

CFLAGS += -I${MOUNT}

WARNS ?= 1

LDADD = -lkiconv

SRCDIRDEP= sbin/mount
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/machine lib/csu/MACHINE_ARCH lib/libc lib/libkiconv stage sys/MACHINE_ARCH/include sys/bsm sys/fs/udf sys/sys
.include <bsd.prog.mk>
