# $FreeBSD$

MOUNT=	${BSDSRCTOP}/sbin/mount
UMNTALL= ${BSDSRCTOP}/usr.sbin/rpc.umntall

.PATH: ${MOUNT} ${UMNTALL}

PROG = umount

SRCS = \
	mounttab.c \
	umount.c \
	vfslist.c

WARNS ?= 0

CFLAGS += -I${MOUNT} -I${UMNTALL}

SRCDIRDEP= sbin/mount usr.sbin/rpc.umntall
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/rpc lib/csu/MACHINE_ARCH lib/libc stage sys/MACHINE_ARCH/include sys/bsm sys/netinet sys/netinet6 sys/nfs sys/rpc sys/sys
.include <bsd.prog.mk>
