# $FreeBSD$

FSCK = ${BSDSRCTOP}/sbin//fsck

.PATH: ${FSCK}

PROG = fsck_msdosfs

SRCS = \
	boot.c \
	check.c \
	dir.c \
	fat.c \
	fsutil.c \
	main.c

WARNS ?= 0

CFLAGS += -I${FSCK}

SRCDIRDEP= sbin/fsck
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/machine lib/csu/MACHINE_ARCH lib/libc stage sys/MACHINE_ARCH/include sys/bsm sys/sys
.include <bsd.prog.mk>
