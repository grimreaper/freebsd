# $FreeBSD$

MOUNT = ${BSDSRCTOP}/sbin/mount

.PATH: ${MOUNT}

PROG = mount_msdosfs

SRCS = \
	getmntopts.c \
	mount_msdosfs.c

WARNS ?= 6

CFLAGS += -I${MOUNT}

LDADD += -lkiconv

SRCDIRDEP= sbin/mount
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/machine lib/csu/MACHINE_ARCH lib/libc lib/libkiconv lib/libutil stage sys/MACHINE_ARCH/include sys/bsm sys/sys
.include <bsd.prog.mk>
