# $FreeBSD$

PROG = rcorder

SRCS = \
	ealloc.c \
	hash.c \
	rcorder.c

WARNS ?= 6

LDADD += -lutil

CFLAGS += -DORDER -I.

GENHDRS += util.h
util.h:
	cp ${BSDSRCTOP}/lib/libutil/libutil.h ${.TARGET}

SRCDIRDEP= lib/libutil
DIRDEP= gnu/lib/csu gnu/lib/libgcc include lib/csu/MACHINE_ARCH lib/libc lib/libutil stage sys/MACHINE_ARCH/include sys/sys
.include <bsd.prog.mk>
