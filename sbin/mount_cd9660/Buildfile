# $FreeBSD$

MOUNT = ${BSDSRCTOP}/sbin/mount

.PATH:	${MOUNT}

PROG = mount_cd9660

SRCS = \
	getmntopts.c \
	mount_cd9660.c

WARNS ?= 6

LDADD += -lkiconv

CFLAGS += -I${MOUNT}

SRCDIRDEP= sbin/mount
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/arpa include/machine lib/csu/MACHINE_ARCH lib/libc lib/libkiconv stage sys/MACHINE_ARCH/include sys/bsm sys/sys
.include <bsd.prog.mk>
