	getmntopts.c \
	mount_ext2fs.c

CFLAGS += -I${MOUNT}

SRCDIRDEP= sbin/mount
DIRDEP= gnu/lib/csu gnu/lib/libgcc include include/machine lib/csu/MACHINE_ARCH lib/libc stage sys/MACHINE_ARCH/include sys/bsm sys/sys
.include <bsd.prog.mk>
