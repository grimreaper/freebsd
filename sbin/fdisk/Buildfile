SRCDIRDEP= sys/geom
DIRDEP= gnu/lib/csu gnu/lib/libgcc include lib/csu/MACHINE_ARCH lib/libc lib/libexpat lib/libgeom lib/libsbuf stage sys/MACHINE_ARCH/include sys/bsm sys/geom sys/sys
.include <bsd.prog.mk>

test:	${PROG}
	sh ${.CURDIR}/runtest.sh
