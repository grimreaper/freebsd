# $FreeBSD$

.PATH:	${BSDSRCTOP}/sys/geom

PROG =	fdisk

SRCS = \
	fdisk.c \
	geom_mbr_enc.c

WARNS ?=	4

LDADD += -lgeom -lbsdxml -lsbuf

